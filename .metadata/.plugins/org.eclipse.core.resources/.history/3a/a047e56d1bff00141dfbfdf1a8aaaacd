package com.epam.newyeargift.entity;

public class Parameter extends Exception {

	public Parameter() {
		super();
	}

	public Parameter(String arg0, Throwable arg1, boolean arg2, boolean arg3) {
		super(arg0, arg1, arg2, arg3);
	}

	public Parameter(String arg0, Throwable arg1) {
		super(arg0, arg1);
	}

	public Parameter(String arg0) {
		super(arg0);
	}

	public Parameter(Throwable arg0) {
		super(arg0);
	}

}
