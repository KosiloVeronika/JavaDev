public class Parser {
	
	private Parser() { }

	public static void parseManager(String text, Composite c) {
		ComponentType compositeType = c.getType();
		switch(compositeType) {
		case TEXT:
			parse(text, compositeType);
			break;
		case SENTENCE:
			parse(text, compositeType);
			break;
		default:
		}

	}
	
	private static void parse(String text, ComponentType cType) {
		
	}
	
}
