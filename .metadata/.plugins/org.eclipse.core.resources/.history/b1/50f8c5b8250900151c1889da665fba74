package parse;

public class Task implements BaseInterface {
	
	private String message = "";
	
	public Task(String message) {
		setMessage(message);
	}

	@Override
	public String handleRequest() {
		return getMessage();
	}

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}

}
