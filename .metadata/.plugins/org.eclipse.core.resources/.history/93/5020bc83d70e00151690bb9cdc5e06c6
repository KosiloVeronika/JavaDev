sentence=((.*?\\{([^}]|(?<!\\n)\})*\\}\\s*?\\n)|((?<=[\\n|^])[\\w\\s]+([-+=/][\\w\\s]+?)+[;]*[\\s]*?\\n)|((\\d\\.)*[\\s\\S]*?(([.?!]+?)|([:]\\s*?\\n)|[\\n](?=\\d\\.|[A-Z]))[\\s]*))
sentenceType =Composite
code=(.*?\\{([^}]|(?<!\\n)\})*\\}\\s*?\\n)|((?<=[\\n|^])[\\w\\s]+([-+=/][\\w\\s]+?)+[;]*[\\s]*?\\n)
codeType =Leaf
word=([\\w-]+)|([\\s]+)|(\\p{Punct}+)
wordType = Leaf