package com.epam.parcerapp.logic;

public class Question implements BaseInterface {

	private String message = "";
	private SubTask root = null;
	
	public Question(SubTask root, String message) {
		setRoot(root);
		setMessage(message);
	}
	
	@Override
	public String handleRequest() {
		// TODO Auto-generated method stub
		return null;
	}

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}

	public SubTask getRoot() {
		return root;
	}

	public void setRoot(SubTask root) {
		this.root = root;
	}

}
