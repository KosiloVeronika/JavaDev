sentence1=(((\\d\\.)*[\\s\\S]*?(([.?!]+?)|([:]\\s*?\\n)|[\\n](?=\\d\\.|[A-Z]))[\\s]*)|(.*?\\{([^}]|(?<!\\n)\})*\\}\\s*?\\n)|((?<=[\\n|^])[\\w\\s]+([-+=/][\\w\\s]+?)+[;]*[\\s]*?\\n))
sentenceType =Composite
code=(.*?\\{([^}]|(?<!\\n)\})*\\}\\s*?\\n)|((?<=[\\n|^])[\\w\\s]+([-+=/][\\w\\s]+?)+[;]*[\\s]*?\\n)
codeType =Leaf
word=([\\w-]+)|([\\s]+)|(\\p{Punct}+)
wordType = Leaf
sentence=((\\d\\.)+[^\\n]*\\n)(?=[A-Z]|(\\d\\.))|(.*?\\{([^}]|(?<!\\n)\\})*\\}\\s*?\\n)|((?<=[\\n|^])[\\w\\s]+([-+=/][\\w\\s]+?)+[;]*[\\s]*?\\n)